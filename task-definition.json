{
	"family": "cloudmapper-%ENVIRONMENT%",
	"containerDefinitions": [
		{
			"image": "255382753382.dkr.ecr.eu-central-1.amazonaws.com/cloudmapper-service:%IMAGE_VERSION%",
			"name": "cloudmapper-%ENVIRONMENT%",
			"cpu": 50,
			"memoryReservation": 256,
			"essential": true,
			"portMappings": [
				{
					"containerPort": 9000,
					"hostPort": 0
				}
			],
			"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-group": "%ENVIRONMENT%",
					"awslogs-region": "eu-central-1",
					"awslogs-stream-prefix": "cloudmapper"
				}
			},
			"environment": [
				{
					"name": "ENVIRONMENT",
					"value": "%ENVIRONMENT%"
				},
				{
					"name": "SERVICE_VERSION",
					"value": "%IMAGE_VERSION%"
				}
			]
		}
	],
	"taskRoleArn": "arn:aws:iam::255382753382:role/credentials-web-xo-%ENVIRONMENT%"
}
