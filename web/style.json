[
    {
        "selector": "node",
        "style": {
            "label": "data(name)",
            "width": 75,
            "height": 75
        }
    },
    
    {
        "selector": "edge",
        "style": {
            "width": 5,
            "opacity": 0.8,
            "line-color": "#888",
            "curve-style": "unbundled-bezier",
            "control-point-distances": 20,
            "control-point-weights": 0.5,
            "target-arrow-shape": "triangle"
        }
    },



    {
        "selector": "edge.admin",
        "style": {
            "width": 10,
            "line-color": "#f00",
            "target-arrow-color": "#f00"
        }
    },
    {
        "selector": "edge.assume_role",
        "style": {
            "line-color": "#999",
            "target-arrow-color": "#999"
        }
    },
    {
        "selector": "edge.assume_role_read",
        "style": {
            "line-color": "#999",
            "target-arrow-color": "#999",
            "line-style": "dashed"
        }
    },
    {
        "selector": "edge.cloudtrail",
        "style": {
            "line-color": "#090",
            "target-arrow-color": "#090"
        }
    },

    {
        "selector": "edge.s3_read",
        "style": {
            "line-color": "#fc0",
            "target-arrow-color": "#fc0",
            "line-style": "dashed"
        }
    },

    {
        "selector": "edge.s3",
        "style": {
            "line-color": "#fc0",
            "target-arrow-color": "#fc0"
        }
    },
    {
        "selector": "edge.vpc",
        "style": {
            "line-color": "#36f",
            "target-arrow-color": "#36f",
            "target-arrow-shape": "none"
        }
    },
    {
        "selector": "edge.directconnect",
        "style": {
            "line-color": "#36f",
            "line-style": "dashed",
            "target-arrow-color": "#36f",
            "target-arrow-shape": "none"
        }
    },
    
    
    
    {
        "selector": "[type = \"hidden\"]",
        "css": {
            "label": "",
            "border-width": 0,
            "background-opacity": 0
        }
    },
    {
        "selector": "[type = \"account\"]",
        "css": {
            "border-color": "#000000",
            "border-width": "1px",
            "border-style": "dashed",
            "background-opacity": 0
        }
    },
    {
        "selector": "[type = \"region\"]",
        "css": {
            "border-color": "#000000",
            "border-style": "solid",
            "background-color": "#c0c0f0",
            "background-opacity": 0.1
        }
    },
    {
        "selector": "[type = \"vpc\"]",
        "css": {
            "border-color": "#6b788c",
            "border-style": "dotted",
            "background-color": "#f09900",
            "background-opacity": 0.1
        }
    },
    {
        "selector": "[type = \"az\"]",
        "css": {
            "border-color": "#f4ad42",
            "border-style": "dashed",
            "background-opacity": 0
        }
    },
    {
        "selector": "[type = \"subnet\"]",
        "css": {
            "border-color": "#ff5959",
            "border-style": "dashed",
            "background-opacity": 0,
            "compound-sizing-wrt-labels": "include"
        }
    },
    
    
    

